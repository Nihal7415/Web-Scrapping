    '''this code is working to login in other site but not for parivahan.govt.in due to captcha. i try to solve captcha problem but all attempt was unsuccessfull'''
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import re\n",
    "url='https://parivahan.gov.in/rcdlstatus/vahan/rcDlHome.xhtml'\n",
    "s=requests.Session()\n",
    "headers={'User-Agent':'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/81.0.4044.122 Safari/537.36'}\n",
    "html=s.get(url,headers=headers)\n",
    "t=BeautifulSoup(html.text,'html.parser')\n",
    "user=input(\"enter your DL \")\n",
    "password=input(\"enter DOB \")\n",
    "k=re.findall('src=\"/rcdlstatus/DispplayCaptcha;jsessionid=(.*?)\"',html.text)\n",
    "g='https://parivahan.gov.in'+'/rcdlstatus/DispplayCaptcha;jsessionid='+k[0]\n",
    "image=s.get(g)\n",
    "f= open('k.jpg','wb')\n",
    "f.write(image.content)\n",
    "f.close()\n",
    "from PIL import Image\n",
    "f=Image.open('k.jpg')\n",
    "f.show()\n",
    "captch=input()\n",
    "j=t.find('input',{'name':'javax.faces.ViewState'})\n",
    "login_data={'form_rcdl': 'form_rcdl',\n",
    "'form_rcdl:tf_dlNO':user,\n",
    "'form_rcdl:tf_dob_input':password,\n",
    "'form_rcdl:j_idt34:CaptchaID':captch,\n",
    "'form_rcdl:javax.faces.ViewState':j['value']}\n",
    "response=s.post(url,login_data,headers=headers)\n"
   ]
  },
